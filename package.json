{
	"name": "openaq-transform",
	"version": "0.0.1",
	"type": "module",
	"packageManager": "pnpm@10.12.1",
	"files": [
		"dist"
	],
	"exports": {
		"./browser": {
			"browser": {
				"types": "./dist/transform-browser/index.d.ts",
				"browser": "./dist/transform-browser/index.js",
				"default": "./dist/transform-browser/index.js"
			},
			"node": null,
			"default": {
				"types": "./dist/transform-browser/index.d.ts",
				"default": "./dist/transform-browser/index.js"
			}
		},
		"./node": {
			"browser": null,
			"node": {
				"types": "./dist/transform-node/index.d.ts",
				"node": "./dist/transform-node/index.js",
				"default": "./dist/transform-node/index.js"
			},
			"default": {
				"types": "./dist/transform-node/index.d.ts",
				"default": "./dist/transform-node/index.js"
			}
		},
		"./core": {
			"types": "./dist/transform-core/index.d.ts",
			"import": "./dist/transform-core/index.js",
			"default": "./dist/transform-core/index.js"
		}
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"fix": "pnpm run format && pnpm run lint",
		"format": "biome check --write .",
		"format:check": "biome check .",
		"lint": "biome lint --write .",
		"lint:check": "biome lint .",
		"test": "vitest run --typecheck",
		"watch:test": "vitest run --typecheck --watch",
		"coverage": "vitest run --typecheck --coverage",
		"type-check": "tsc --noEmit",
		"type-check:watch": "tsc --noEmit --watch"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.12",
		"@types/debug": "^4.1.12",
		"@types/geojson": "^7946.0.16",
		"@types/luxon": "^3.6.2",
		"@types/node": "^24.0.1",
		"@types/proj4": "^2.5.6",
		"@vitest/coverage-v8": "^3.2.4",
		"happy-dom": "^17.4.6",
		"msw": "^2.7.5",
		"oxlint-tsgolint": "^0.2.0",
		"tsdown": "^0.15.4",
		"typedoc": "^0.28.5",
		"typescript": "~5.8.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@jmespath-community/jmespath": "^1.3.0",
		"csv-parse": "^5.6.0",
		"debug": "^4.4.1",
		"luxon": "^3.6.1",
		"proj4": "^2.15.0"
	}
}
